<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="models.TaskTags" table="TaskTags" schema="dbo" catalog="TestDatabase">
        <id name="tagLinkId">
            <column name="TagLinkId" sql-type="int"/>
        </id>
        <property name="tagId">
            <column name="TagId" sql-type="int"/>
        </property>
        <property name="taskId">
            <column name="TaskId" sql-type="int"/>
        </property>
        <many-to-one name="tagsByTagId" class="models.Tags">
            <column name="TagId"/>
        </many-to-one>
        <many-to-one name="tasksByTaskId" class="models.Tasks">
            <column name="TaskId"/>
        </many-to-one>
    </class>
</hibernate-mapping>