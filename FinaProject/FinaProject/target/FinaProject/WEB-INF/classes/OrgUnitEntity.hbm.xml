<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="models.OrgUnitEntity" table="OrgUnit" schema="dbo" catalog="TestDatabase">
        <id name="orgUnitId">
            <column name="OrgUnitId" sql-type="int"/>
        </id>
        <property name="orgUnitFullName">
            <column name="OrgUnitFullName" sql-type="varchar(100)" length="100" not-null="true"/>
        </property>
        <property name="orgUnitShartName">
            <column name="OrgUnitShartName" sql-type="varchar(30)" length="30" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>