<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="model.PacksSendings" table="PacksSendings" schema="dbo" catalog="TestDatabase">
        <id name="sendingId">
            <column name="SendingId" sql-type="int"/>
        </id>
        <property name="packId">
            <column name="PackId" sql-type="int"/>
        </property>
        <property name="sendingDate">
            <column name="SendingDate" sql-type="datetime" not-null="true"/>
        </property>
        <property name="sendingEmail">
            <column name="SendingEmail" sql-type="varchar(100)" length="100" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>