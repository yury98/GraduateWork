<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="model.TasksOfPacks" table="TasksOfPacks" schema="dbo" catalog="TestDatabase">
        <id name="tasksOfPacksId">
            <column name="TasksOfPacksId" sql-type="int"/>
        </id>
        <property name="taskId">
            <column name="TaskId" sql-type="int"/>
        </property>
        <property name="packId">
            <column name="PackId" sql-type="int"/>
        </property>
    </class>
</hibernate-mapping>